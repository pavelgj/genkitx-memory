{
  "name": "genkitx-memory",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "ai",
    "memory"
  ],
  "version": "0.1.0",
  "description": "A Genkit plugin for managing and interacting with long-term memory.",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "run-sample": "MEMORY_FILE_PATH=`pwd`/memory.local.json genkit start -- tsx samples/index.ts",
    "build:clean": "rimraf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "node --import tsx --test ./tests/*_test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pavelgj/genkitx-memory"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "peerDependencies": {
    "genkit": "^1.14.1"
  },
  "devDependencies": {
    "@genkit-ai/googleai": "^1.14.1",
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
